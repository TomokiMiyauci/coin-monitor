rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /markets/{market} {
      allow create, read;

      match /pairs/{pair} {
        allow create, read;

        match /rates/{rate} {
          allow create, read;
        }
      }
    }
  }
}
